<div class="row">
  <div class="col-6">{{this.name}} {{this.lastName}}</div>
  <div class="col-3">
    <button
      type="button"
      class="btn btn-primary"
      data-toggle="modal"
      data-target="#modifyClientModal{{this._id}}"
    >
      <i class="fas fa-user-edit"></i>
    </button>
  </div>
  <div class="col-3">
    <button
      type="button"
      class="btn btn-primary"
      data-toggle="modal"
      data-target="#deleteClientModal{{this._id}}"
    >
      <i class="fas fa-user-times"></i>
    </button>
  </div>
</div>